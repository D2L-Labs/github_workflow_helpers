{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "overrides": [
        {
            "files": [
                "src/**/*.js"
            ]
        }
    ],
    "rules": {
        "no-console": "off",
        "max-len": [
            "error",
            {
                "code": 120
            }
        ]
    }
}
